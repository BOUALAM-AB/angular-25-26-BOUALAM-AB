{
  "name": "assignment-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng.js",
    "build": "npm run ng -- build",
    "start": "node server.js",
    "watch": "npm run ng -- build --watch --configuration development",
    "test": "npm run ng -- test"
  },

  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.0",
    "@angular/cdk": "20.2.4",
    "@angular/common": "20.3.0",
    "@angular/compiler": "20.3.0",
    "@angular/core": "20.3.0",
    "@angular/forms": "20.3.0",
    "@angular/material": "20.2.4",
    "@angular/platform-browser": "20.3.0",
    "@angular/platform-browser-dynamic": "20.3.0",
    "@angular/router": "20.3.0",
    "@angular/cli": "20.3.2",
    "@angular-devkit/build-angular": "20.3.2",
    "express": "4.18.2",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "20.3.0",
    "@types/express": "4.17.17",
    "@types/jasmine": "5.1.0",
    "@types/node": "20.17.19",
    "jasmine-core": "5.9.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "typescript": "5.9.2"
  }
}
